{
  "title": "Change left command",
  "rules": [
    {
      "description": "Change left command to $ if pressed alone",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_gui",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_gui"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "4",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        }
      ]
    }
  ]
}
